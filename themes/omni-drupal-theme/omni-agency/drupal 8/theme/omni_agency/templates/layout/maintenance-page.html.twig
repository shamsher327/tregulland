<div id="page">  
  <header id="header">
    <div id="header-bottom">
      <div class="inner">
        <div id="site-info">        
          {% if logo %}
            <div id="logo">
              <a href="{{ path('<front>') }}" title="{{ 'Home'|t }}" rel="home">
                <img src="{{ logo }}" alt="{{ 'Home'|t }}" />
              </a>
            </div>
          {% else %}
            <div id="site-name">
              <h1><a href="{{ path('<front>') }}" title="{{ 'Home'|t }}" rel="home">{{ site_name }}</a></h1>
            </div>
          {% endif %}
          {% if site_slogan %}
            <div id="site-slogan">
              {{ site_slogan }}
            </div>
          {% endif %}
        </div>        
      </div>
    </div>    
  </header> 
  <div id="main">           
    <div id="main-middle" class="row">
      <div id="primary" class="{{primary_width}}column">         
        <div id="content" class="clearfix">
          {{ page.content }}
        </div>                        
      </div> 
    </div>
  </div> 
</div>
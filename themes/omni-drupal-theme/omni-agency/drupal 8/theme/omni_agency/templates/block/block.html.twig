{% set classes %}block block-{{ configuration.provider|clean_class }} {{ plugin_id|clean_class }}{% endset %} 

<div id="{{ attributes.id }}" class="{{ classes }} {{ attributes.class }}">
  <div class="inner">
    {{ title_prefix }}
    {% if label %}
      <div class="title">
        <h3{{ title_attributes }}>{{ label }}</h3>
      </div>
    {% endif %}
    {{ title_suffix }}
    {% block content %}
      <div{{ content_attributes.addClass('content clearfix') }}>
        {{ content }}
      </div>
    {% endblock %}
  </div>
</div>
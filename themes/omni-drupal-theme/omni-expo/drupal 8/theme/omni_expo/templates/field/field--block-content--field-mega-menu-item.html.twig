{% for item in items %}  
  {% set block = item.content['#block_content'] %}
  {% set url = block.field_link.uri %}
  {% set title = block.field_link.title %}  
  {% if 'internal' in url %}
    {% set url %}{{ url('<front>') }}{{ url[10:] }}{% endset %}
  {% endif %}
  <li>
    <a href="{{ url }}">{{ title }}</a>
    <ul class="mega-menu">
      <li>
        {{ item.content.field_mega_menu_element_item }} 
      </li>
    </ul> 
  </li>
{% endfor %}
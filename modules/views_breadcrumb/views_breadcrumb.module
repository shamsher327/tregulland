<?php

/**
 * @file
 * Contains views_breadcrumb.module..
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function views_breadcrumb_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the views_breadcrumb module.
    case 'help.page.views_breadcrumb':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Setting bread crumbs for all the view&#039;s pages.') . '</p>';
      return $output;

    default:
  }
}
